#!/bin/bash

DIR="$(dirname "$0")"
. "${DIR}/config"


for SERVICE_REGION in "${SERVICE_REGIONS[@]}"
do
    SERVICE_URL=$(gcloud beta run services describe $SERVICE_NAME \
        --platform managed \
        --region $SERVICE_REGION \
        --format="value(status.domain)")

    echo "bin/client --target ${SERVICE_URL/https:\/\//}:443"
done


